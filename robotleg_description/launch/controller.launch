<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find robotleg_description)/config/trajectory_controller.yaml"
            command="load"/>

  <!-- Load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner"
        respawn="false" output="screen" ns="/robotleg"
        args="joint_state_controller
              robotleg_controller">
  </node>

  <!-- Convert joint states to TF transforms for rviz, etc -->
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" ns="/robotleg">
  </node>

  <!-- rqt ->
  <node name="rqt_joint_trajectory_controller" pkg="rqt_gui" type="rqt_gui" args="-s rqt_joint_trajectory_controller" ns="/robotleg"/>
  <node name="rqt_reconfigure" pkg="rqt_gui" type="rqt_gui" args="-s rqt_reconfigure"/>
  <node name="rqt_publisher" pkg="rqt_gui" type="rqt_gui" args="-s rqt_publisher"/>
  -->
</launch>